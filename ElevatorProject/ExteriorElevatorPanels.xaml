    >
            <Label.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="0.201"/>
                    <TranslateTransform X="0.345" Y="0.16"/>
                </TransformGroup>
            </Label.RenderTransform>
        </Label>
        <Label x:Name="label" 

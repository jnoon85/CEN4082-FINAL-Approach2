    >
            <Label.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="0.201"/>
                    <TranslateTransform X="0.361" Y="-0.082"/>
                </TransformGroup>
            </Label.RenderTransform>
        </Label>
        <Label x:Name="display1" 
    		Content="1" 
    		FontSize="32"
    		Foreground="Red"
    		HorizontalAlignment="Left" 
    		VerticalAlignment="Top" 
    		HorizontalContentAlignment="Center" 
    		VerticalContentAlignment="Center" 
    		Margin="76,71,0,0"
    		BorderThickness="2"
    		BorderBrush="Black"
    		Height="77" 
    		Width="71" 
    		RenderTransformOrigin="0.515,0.49">
